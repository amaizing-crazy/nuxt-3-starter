{
  "routes": [
    {
      "route": "/index.html",
      "redirect": "/"
    },
    {
      "route": "/old-route",
      "redirect": "/new-route",
      "statusCode": 301
    },
    {
      "route": "/specials",
      "redirect": "/deals",
      "statusCode": 301
    },
    {
      "route": "/special",
      "redirect": "/deal",
      "statusCode": 302
    },
    {
      "route": "/spec",
      "redirect": "/dea"
    },
    {
      "route": "/",
      "rewrite": "/api/server"
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "navigationFallback": {
    "rewrite": "/api/server"
  }
}
